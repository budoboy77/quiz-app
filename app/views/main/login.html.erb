<div class="well">
	<H1>Login</H1>
	<p>
		<div "span12 pagination-centered">
		Enter your email address.<br>
		<%== text_field_tag :email, "", size: 40 %>
		</div>
	</p>
	<p>
		Enter your password.<br>
		<%== text_field_tag :password, "", size: 40 %>
	</p>
	<p>
		<%== submit_tag "Log in" %>
	</p>
</div>